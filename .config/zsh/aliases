# python poetry
alias psh="poetry shell"
alias padd="poetry add"
alias pinstall="poetry install"
alias premove="poetry remove"
alias pupdate="poetry update"

# nvim
alias vi="nvim"

# mosh
alias mosh="mosh-with-jump"

# bat
alias bat="batcat"

# hp
alias hp-proxy="tsh -t corvallis -NH proxy 15.0.0.0/24"
